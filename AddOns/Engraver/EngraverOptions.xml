<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	<Script file="EngraverOptionsFilterControl.lua"/>
	<Script file="EngraverOptions.lua"/>
	<CheckButton name="EngraverOptionsFilterRuneButtonTemplate" mixin="EngraverOptionsFilterRuneButtonMixin" inherits="EngraverRuneButtonTemplate" virtual="true">
		<Scripts>
			<OnClick method="OnClick"/>
		</Scripts>
	</CheckButton>
	<Frame name="EngraverOptionsFilterCategoryFrameTemplate" mixin="EngraverOptionsFilterCategoryFrameMixin" inherits="EngraverCategoryFrameTemplate" virtual="true">
		<Scripts>
			<OnLoad method="OnLoad"/>
		</Scripts>
	</Frame>
	<Button name="EngraverOptionsFilterListButtonTemplate" mixin="EngraverOptionsFilterListButtonMixin" virtual="true">
		<Size x="175" y="20"/>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture parentKey="Texture">
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<FontString parentKey="Label" inherits="GameFontNormal" justifyH="LEFT">
					<Anchors>
						<Anchor point="TOPLEFT" x="36" y="1"/>
						<Anchor point="BOTTOMRIGHT" y="1"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnEnter method="OnEnter"/>
			<OnLeave method="OnLeave"/>
			<OnMouseDown method="OnMouseDown"/>
			<OnMouseUp method="OnMouseUp"/>
			<OnClick method="OnClick"/>
		</Scripts>
	</Button>
	<Frame name="EngraverOptionsFilterEditorTemplate" inherits="SettingsListElementTemplate" mixin="EngraverOptionsFilterEditorMixin" virtual="true">
		<Size x="280" y="120"/>
		<Frames>
			<Frame name="$parentFilterList" parentKey="filterList" inherits="SettingsCategoryListTemplate" mixin="EngraverOptionsFilterListMixin">
				<Size x="230" y="120"/>
				<Anchors>
					<Anchor x="0"/>
				</Anchors>
				<Layers>
					<Layer level="BACKGROUND">
						<Texture parentKey="backgroundTexture" atlas="collections-itemborder-uncollected">
							<Anchors>
								<Anchor point="TOPLEFT" x="10" y="8"/>
								<Anchor point="BOTTOMRIGHT" y="-8"/>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
				<Scripts>
					<OnLoad method="OnLoad"/>
				</Scripts>
			</Frame>
			<Button name="$parentNewButton" parentKey="NewButton" inherits="UIPanelButtonTemplate" text="New Filter">
				<Size x="100" y="21"/>
				<Anchors>
					<Anchor point="TOP" relativeTo="$parentFilterList" relativePoint="BOTTOM" y="-10"/>
				</Anchors>
			</Button>
			<Button name="$parentDeleteButton" parentKey="DeleteButton" inherits="UIPanelButtonTemplate" text="Delete">
				<Size x="100" y="21"/>
				<Anchors>
					<Anchor point="TOP" relativeTo="$parentNewButton" relativePoint="BOTTOM"/>
				</Anchors>
			</Button>
			<Frame name="$parentEquipmentSlotsFrame" parentKey="equipmentSlotsFrame" mixin="EngraverOptionsFilterEquipmentSlotsMixin">
				<Size x="40" y="40"/>
				<Anchors>
					<Anchor point="TOPLEFT" relativePoint="TOP" x="-80" y="0"></Anchor>
				</Anchors>
				<Frames>
					<Frame name="$parentChestFrame" parentKey="chestFrame" inherits="EngraverOptionsFilterCategoryFrameTemplate">
						<Size x="40" y="40"/>
						<Anchors>
							<Anchor></Anchor>
						</Anchors>
					</Frame>
					<Frame name="$parentHandsFrame" parentKey="handsFrame" inherits="EngraverOptionsFilterCategoryFrameTemplate">
						<Anchors>
							<Anchor point="TOPLEFT" relativeTo="$parentChestFrame" relativePoint="BOTTOMLEFT">
							</Anchor>
						</Anchors>
					</Frame>
					<Frame name="$parentLegsFrame" parentKey="legsFrame" inherits="EngraverOptionsFilterCategoryFrameTemplate">
						<Anchors>
							<Anchor point="TOPLEFT" relativeTo="$parentHandsFrame" relativePoint="BOTTOMLEFT">
							</Anchor>
						</Anchors>
					</Frame>
				</Frames>
				<Scripts>
					<OnLoad method="OnLoad"/>
				</Scripts>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
		</Scripts>
	</Frame>
	<Frame name="EngraverOptionsFrame" mixin="EngraverOptionsFrameMixin">
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnEvent method="OnEvent"/>
		</Scripts>
		<Frames>
			<Frame parentKey="settingsList" inherits="SettingsListTemplate" setAllPoints="true"/>
		</Frames>
	</Frame>
</Ui>
